<script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
<script src="/javascripts/graph.js"></script>

<script>
  // backend data from route:
  const graphData = {{{ json graph }}};
  // graph = { mbappe:{season:{goals,assists,gplus}, allTime:{...}}, haaland:{...}, vinicius:{...} }

  // initialize reusable graph
  initCumulativeGraph({
    containerId: "progressGraph",   // <div id="progressGraph">
    wrapperId: "chartWrapper",      // outer div with fade animation

    graphData,                      // real data from backend

    modeConfig: {
      key1: "filter1",               // this page = "season" vs "allTime"
      key2: "filter2",
      defaultKey: "filter1",
      btn1Id: "seasonBtn",
      btn2Id: "allTimeBtn"
    },

    statConfig: {
      keys: ["goals", "gplus", "assists"],
      defaultKey: "goals",
      buttons: {
        goals:   "goalsBtn",
        gplus:   "gplusBtn",
        assists: "assistsBtn"
      }
    }
  });
</script>
