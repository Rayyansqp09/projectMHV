                <td class="stat-text">{{mbappe.GoalRatio}}<br><span class="stat-name">GOAL RATIO</span></td>
                <td class="stat-text">{{haaland.GoalRatio}}<br><span class="stat-name">GOAL RATIO</span></td>
                <td class="stat-text">{{vini.GoalRatio}}<br><span class="stat-name">GOAL RATIO</span></td>
            </tr>
            <tr>
                <td class="stat-text">{{mbappe.Assists}}<br><span class="stat-name">ASSISTS</span></td>
                <td class="stat-text">{{haaland.Assists}}<br><span class="stat-name">ASSISTS</span></td>
                <td class="stat-text">{{vini.Assists}}<br><span class="stat-name">ASSISTS</span></td>
            </tr>

            <tr>
                <td class="stat-text">{{mbappe.CC}}<br><span class="stat-name">CHANCE CREATED</span></td>
                <td class="stat-text">{{haaland.CC}}<br><span class="stat-name">CHANCE CREATED</span></td>
                <td class="stat-text">{{vini.CC}}<br><span class="stat-name">CHANCE CREATED</span></td>
            </tr>
            <tr>
                <td class="stat-text">{{mbappe.BCC}}<br><span class="stat-name">BIG CHANCE CREATED</span></td>
                <td class="stat-text">{{haaland.BCC}}<br><span class="stat-name">BIG CHANCE CREATED</span></td>
                <td class="stat-text">{{vini.BCC}}<br><span class="stat-name">BIG CHANCE CREATED</span></td>
            </tr>
             <tr>
                <td class="stat-text">{{mbappe.ShotOnTarget}}<br><span class="stat-name">SHOT ON TARGET</span></td>
                <td class="stat-text">{{haaland.ShotOnTarget}}<br><span class="stat-name">SHOT ON TARGET</span></td>
                <td class="stat-text">{{vini.ShotOnTarget}}<br><span class="stat-name">SHOT ON TARGET</span></td>
            </tr>
            <tr>
                <td class="stat-text">{{mbappe.Dribbles}}<br><span class="stat-name">DRIBBLES</span></td>
                <td class="stat-text">{{haaland.Dribbles}}<br><span class="stat-name">DRIBBLES</span></td>
                <td class="stat-text">{{vini.Dribbles}}<br><span class="stat-name">DRIBBLES</span></td>
            </tr>


        </tbody>
    </table>

    {{#if admin}}
    <button class="Dt-btn" onclick="toggleModal(this)" data-stat="" table="all_time">UPDATE</button>
    {{/if}}
</div>
</div>

